{
  "devDependencies": {
    "prettier": "^3.3.3"
  },
  "scripts": {
    "az:login": "az login --scope api://trapi/.default",
    "genai": "npx --yes genaiscript run",
    "gcm": "npx --yes genaiscript run gcm",
    "serve": "npx --yes genaiscript serve",
    "build": "npx --yes genaiscript scripts compile",
    "promptpex": "npx --yes genaiscript run promptpex",
    "promptpex:speech-tag": "npx --yes genaiscript run promptpex \"samples/speech-tag/speech-tag.prompty\" --vars \"evals=true\" --vars \"models=github:gpt-4o-mini;github:Phi-3-5-mini-instruct\"",
    "promptpex:speech-tag:azure": "npx --yes genaiscript run promptpex \"samples/speech-tag/speech-tag.prompty\" --vars \"evals=true\" --model \"azure:gpt-4o_2024-08-06\" --vars \"models=azure:gpt-4o-mini_2024-07-18;azure:gpt-35-turbo_1106\" --vars \"out=evals/v10\"",
    "promptpex:demo": "npx --yes genaiscript run promptpex \"samples/demo/demo.prompty\" --vars \"evals=true\" --vars \"models=github:gpt-4o-mini;github:Phi-3-5-mini-instruct\"",
    "promptpex:demo:azure": "npx --yes genaiscript run promptpex \"samples/demo/demo.prompty\" --vars \"evals=true\" --model \"azure:gpt-4o_2024-08-06\" --vars \"models=azure:gpt-4o-mini_2024-07-18;azure:gpt-35-turbo_1106\"",
    "promptpex:paper": "npx --yes genaiscript run promptpex \"samples/speech-tag/speech-tag.prompty\" \"samples/text-to-p/text-to-p.prompty\" \"samples/openai-examples/elements.prompty\" \"samples/big-prompt-lib/art-prompt.prompty\" \"samples/prompt-guide/extract-names.prompty\" \"samples/text-classification/classify-input-text.prompty\" \"samples/big-prompt-lib/sentence-rewrite.prompty\" \"samples/azure-ai-studio/shakespearean-writing-assistant.prompty\" --vars \"evals=true\" --vars \"models=azure:gpt-4o-mini_2024-07-18;ollama:gemma2:9b;ollama:qwen2.5:3b;ollama:llama3.2:1b\" --vars \"out=evals/v7\"",
    "promptpex:v9": "npx --yes genaiscript run promptpex \"samples/speech-tag/speech-tag.prompty\" \"samples/text-to-p/text-to-p.prompty\" \"samples/openai-examples/elements.prompty\" \"samples/big-prompt-lib/art-prompt.prompty\" \"samples/prompt-guide/extract-names.prompty\" \"samples/text-classification/classify-input-text.prompty\" \"samples/big-prompt-lib/sentence-rewrite.prompty\" \"samples/azure-ai-studio/shakespearean-writing-assistant.prompty\" --vars \"evals=true\" --vars \"models=azure:gpt-4o-mini_2024-07-18;ollama:gemma2:9b;ollama:qwen2.5:3b;ollama:llama3.2:1b\" --vars \"out=evals/v9\"",
    "promptpex:o1mini": "npx --yes genaiscript run promptpex \"samples/speech-tag/speech-tag.prompty\" \"samples/text-to-p/text-to-p.prompty\" \"samples/openai-examples/elements.prompty\" \"samples/big-prompt-lib/art-prompt.prompty\" \"samples/prompt-guide/extract-names.prompty\" \"samples/text-classification/classify-input-text.prompty\" \"samples/big-prompt-lib/sentence-rewrite.prompty\" \"samples/azure-ai-studio/shakespearean-writing-assistant.prompty\" --vars \"evals=true\" --vars \"models=azure:gpt-4o-mini_2024-07-18;ollama:gemma2:9b;ollama:qwen2.5:3b;ollama:llama3.2:1b\" --vars \"out=evals/v8\""
  }
}
